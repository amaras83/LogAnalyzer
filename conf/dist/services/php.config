<services>
    <service name="php" title="PHP Log Summary">
        <logfiles>
            <file src="/var/log/httpd/error_log" />
            <file src="/var/log/apache/error_log" />
            <file src="C:\\Users\\Public\\apache\\logs\\error.log" />
        </logfiles>
        
        <parsers>
            <parser name="PHPParser" />
        </parsers>
    </service>
</services>
